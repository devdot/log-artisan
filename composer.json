{
    "name": "devdot/log-artisan",
    "description": "View and manage log files using the artisan CLI",
    "keywords": [
        "log artisan",
        "log management",
        "log view",
        "devdot",
        "laravel"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Devdot\\LogArtisan\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Thomas Kuschan",
            "email": "mail@thomaskuschan.de"
        }
    ],
    "require": {
        "php": "^8.2",
        "devdot/monolog-parser": "^1.5",
        "illuminate/contracts": "^11",
        "illuminate/console": "^11"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Devdot\\LogArtisan\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "captainhook/captainhook": "^5.16",
        "squizlabs/php_codesniffer": "^3.9"
    },
    "scripts": {
        "check-static": "phpstan",
        "format": "phpcbf --standard=PSR12 src",
        "post-autoload-dump": [
            "vendor/bin/captainhook install -f -s"
        ]
    }
}
